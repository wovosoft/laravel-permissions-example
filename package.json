        "jquery": "^3.5",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "popper.js": "^1.12",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.20.1",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.13.0",
        "axios": "^0.21",
        "bootstrap": "^4.0.0",
        "bootstrap-vue": "^2.9.0",
        "vue": "^2.5.17"
    }
